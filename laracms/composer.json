        "anchnet/aliyun-openapi-php-sdk": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "orangehill/iseed": "^2.5",
        "overtrue/laravel-query-logger": "^1.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Wanglelecc\\Laracms\\": "src/"
        },
        "files": [
            "src/Support/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Models\\": "tests/models",
            "Tests\\Controllers\\": "tests/controllers"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "Wanglelecc\\Laracms\\Providers\\AliyunOssServiceProvider",
                "Wanglelecc\\Laracms\\Providers\\AppServiceProvider",
                "Wanglelecc\\Laracms\\Providers\\AuthServiceProvider",
                "Wanglelecc\\Laracms\\Providers\\AzureStorageServiceProvider",
                "Wanglelecc\\Laracms\\Providers\\EasySmsServiceProvider",
                "Wanglelecc\\Laracms\\Providers\\EventServiceProvider",
                "Wanglelecc\\Laracms\\Providers\\LaracmsServiceProvider",
                "Wanglelecc\\Laracms\\Providers\\RouteServiceProvider",
                "Wanglelecc\\Laracms\\Providers\\ScoutServiceProvider"
            ]
        }
    }
}
